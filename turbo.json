{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["build/**", "dist/**", "esm/**"]
    },
    "dev": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^dev"],
      "persistent": true,
      "cache": false
    },
    "fix": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^fix"],
      "outputs": ["build/**", "dist/**", "esm/**"]
    }
  }
}
