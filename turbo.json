{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "esm/**"]
    },
    "fix": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^fix"]
    },
    "check": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^build", "^check"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "esm/**"]
    },
    "check:type": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^build", "^check:type"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "esm/**"]
    },
    "check:lint": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^check:lint"]
    },
    "check:format": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^check:format"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
