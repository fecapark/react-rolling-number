{
  "name": "react-rolling-number",
  "author": "fecapark@gmail.com",
  "version": "1.1.1",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fecapark/react-rolling-number.git"
  },
  "keywords": [
    "react",
    "rolling-number",
    "rolling",
    "number",
    "component",
    "animation",
    "animated",
    "motion",
    "library"
  ],
  "sideEffects": false,
  "access": "public",
  "main": "./dist/index.cjs",
  "module": "./esm/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/**/*",
    "esm/**/*"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsc -b && tsdown",
    "publish": "pnpm build && pnpm publish --access public",
    "fix": "pnpm fix:lint && pnpm fix:format",
    "fix:lint": "eslint . --fix",
    "fix:format": "prettier --write .",
    "check": "pnpm check:lint && pnpm check:format && pnpm check:type",
    "check:lint": "eslint .",
    "check:format": "prettier . --list-different",
    "check:type": "tsc --noEmit"
  },
  "devDependencies": {
    "@react-rolling-number/eslint-config": "workspace:*",
    "@react-rolling-number/typescript-config": "workspace:*",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vanilla-extract/css": "^1.17.5",
    "@vanilla-extract/rollup-plugin": "^1.5.0",
    "eslint": "catalog:",
    "eslint-plugin-react-refresh": "^0.4.24",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tsdown": "^0.17.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "es-toolkit": "^1.42.0",
    "from-exponential": "^1.1.1",
    "motion": "^12.23.24",
    "react-simplikit": "^0.0.46"
  }
}
