{
  "name": "react-rolling-number",
  "author": "fecapark@gmail.com",
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fecapark/react-rolling-number.git"
  },
  "keywords": [
    "react",
    "rolling-number",
    "rolling",
    "number",
    "component",
    "animation",
    "library"
  ],
  "sideEffects": false,
  "access": "public",
  "main": "./dist/index.cjs",
  "module": "./esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./esm/index.js",
        "types": "./esm/index.d.ts"
      },
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      }
    }
  },
  "files": [
    "dist/**/*",
    "esm/**/*"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsc -b && tsup",
    "fix": "pnpm fix:lint && pnpm fix:format",
    "fix:lint": "eslint . --fix",
    "fix:format": "prettier --write ."
  },
  "devDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@react-rolling-number/eslint-config": "workspace:*",
    "@react-rolling-number/typescript-config": "workspace:*",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.29.0",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "react": "*"
  }
}
